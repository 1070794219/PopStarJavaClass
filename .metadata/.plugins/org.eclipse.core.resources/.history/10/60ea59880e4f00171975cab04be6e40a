package cn.campsg.practical.bubble.entity;

public class Star {
	public enum StarType{
		BLUE(0),GREEN(1),YELLOW(2),RED(3),PURPLE(4);
		
		private int index;
		
		private StarType(){
			
		}
		
		private StarType(int index){
			this.index = index;
		} 
		
		public int value(){
			return this.index;
		}
	}
	public static void main(String[] args){
		System.out.println(StarType.BLUE.value());
	}
}
